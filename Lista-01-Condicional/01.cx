#include <stdio.h>
#include <stdlib.h>
int main(){
    float salariomin,salario;
    int conta;
    printf("Qual e o salario minimo?\n");
    scanf("%f",&salariomin);
    printf("Qual seu salario?\n");
    scanf("%f",&salario);
    conta=salario/salariomin;
    printf("O quantidade de salario minimo e de %d",conta);
    return 0;
}
